# FleetCmD Environment Configuration
# Copy this file to .env and fill in your values

# Database Configuration
# Format: postgresql://username:password@host:port/database
# Replace YOUR_PASSWORD with your PostgreSQL password
DATABASE_URL=postgresql://postgres:YOUR_PASSWORD@localhost:5432/fleetcmd

# Session Secret - Change this to a random string for security
SESSION_SECRET=FleetCmD2025SecureSessionKey

# Environment (development or production)
NODE_ENV=production

# Port
PORT=5000
